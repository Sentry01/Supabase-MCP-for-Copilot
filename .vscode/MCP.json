{

    "inputs": [],
    "servers": {
        "mcp-server-time": {
            "command": "python",
            "args": [
                "-m",
                "mcp_server_time",
                "--local-timezone=America/Los_Angeles"
            ],
            "env": {}
        },
        "github": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-github"
            ],
            "env": {
                "GITHUB_TOKEN": "[REDACTED]"
            }
        },
        "supabase-server": {
            "command": "npx",
            "args": [
                "-y",
                "supabase"
            ],
            "env": {
                "SUPABASE_URL": "your-supabase-url",
                "SUPABASE_KEY": "your-supabase-key"
            },
            "type": "stdio"
        }
    }
}